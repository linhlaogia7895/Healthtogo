<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MainPageServlet</servlet-name>
        <servlet-class>main.MainPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsersServlet</servlet-name>
        <servlet-class>users.UsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NutritionLibrary</servlet-name>
        <servlet-class>main.NutritionLibrary</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminPageServlet</servlet-name>
        <servlet-class>admin.AdminPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientInformation</servlet-name>
        <servlet-class>users.PatientInformation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminPageHelper</servlet-name>
        <servlet-class>admin.AdminPageHelper</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageStaffServlet</servlet-name>
        <servlet-class>admin.ManageStaffServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainPageServlet</servlet-name>
        <url-pattern>/MainPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsersServlet</servlet-name>
        <url-pattern>/UsersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NutritionLibrary</servlet-name>
        <url-pattern>/NutritionLibrary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminPageServlet</servlet-name>
        <url-pattern>/AdminPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientInformation</servlet-name>
        <url-pattern>/PatientInformation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminPageHelper</servlet-name>
        <url-pattern>/AdminPageHelper</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageStaffServlet</servlet-name>
        <url-pattern>/ManageStaffServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>MainPageServlet</welcome-file>
    </welcome-file-list>
</web-app>
