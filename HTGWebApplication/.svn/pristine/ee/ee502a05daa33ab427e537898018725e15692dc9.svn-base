package domains.users;

import java.sql.Date;

/**
 *
 * @author 696645
 */
public final class Journal {
    private int journalNumber;
    private int patientId;
    private int recordNumber;
    private Date dateRecord;
    private int dietPlanNumber;
    private String motivation;
    private boolean activityCompleted;
    private double startWeight;
    private double endWeight;

    public Journal() {
    }

    public Journal(int journalNumber, int patientId, int recordNumber, Date dateRecord, int dietPlanNumber, String motivation, boolean activityCompleted, double startWeight, double endWeight) {
        this.journalNumber = journalNumber;
        this.patientId = patientId;
        this.recordNumber = recordNumber;
        this.dateRecord = dateRecord;
        this.dietPlanNumber = dietPlanNumber;
        this.motivation = motivation;
        this.activityCompleted = activityCompleted;
        this.startWeight = startWeight;
        this.endWeight = endWeight;
    }

    public int getJournalNumber() {
        return journalNumber;
    }

    public int getPatientId() {
        return patientId;
    }

    public int getRecordNumber() {
        return recordNumber;
    }

    public Date getDateRecord() {
        return dateRecord;
    }

    public int getDietPlanNumber() {
        return dietPlanNumber;
    }

    public String getMotivation() {
        return motivation;
    }

    public boolean isActivityCompleted() {
        return activityCompleted;
    }

    public double getStartWeight() {
        return startWeight;
    }

    public double getEndWeight() {
        return endWeight;
    }
    
}
