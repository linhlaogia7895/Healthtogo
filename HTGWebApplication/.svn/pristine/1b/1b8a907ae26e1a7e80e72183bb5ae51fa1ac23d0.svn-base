<!--search box - style in css file-->
    <form>
	<div class="w3-row">
	    <div class="form-group w3-col m1 pull-left">
		<input ng-model="SEARCHDATE" 
		       type="text" 
		       id="datepicker" 
		       class="form-control container-fluid" 
		       name="DATE_SEARCH" 
		       placeholder="Enter Date" 
		       style="width: 100px;">
	    </div>
	    <div class="form-group w3-col m2">
		<button class="btn btn-success">Check Date</button>
	    </div>
	</div>
    </form>
    <br>

    <!--List of schedules-->
